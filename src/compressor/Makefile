COMPRESSORS = VPC BDI FPC CPACK BPC PATTERN

VPCMODULES_SRCDIR = VPCmodules
VPCMODULES_SRCS = $(wildcard $(VPCMODULES_SRCDIR)/*.cpp)
VPCMODULES_OBJS = $(VPCMODULES_SRCS:$(VPCMODULES_SRCDIR)/%.cpp=$(OBJDIR)/%.o)

all : $(COMPRESSORS)

BDI : 
	@$(CC) $(CFLAGS) -c -o $(OBJDIR)/BDI.o BDI.cpp $(LDFLAGS)

FPC :
	@$(CC) $(CFLAGS) -c -o $(OBJDIR)/FPC.o FPC.cpp $(LDFLAGS)

CPACK :
	@$(CC) $(CFLAGS) -c -o $(OBJDIR)/CPACK.o CPACK.cpp $(LDFLAGS)

PATTERN :
	@$(CC) $(CFLAGS) -c -o $(OBJDIR)/PATTERN.o Pattern.cpp $(LDFLAGS)

BPC :
	@$(CC) $(CFLAGS) -c -o $(OBJDIR)/BPC.o BPC.cpp $(LDFLAGS)

VPC : $(VPCMODULES_OBJS)
	@$(CC) $(CFLAGS) -c -o $(OBJDIR)/VPC.o VPC.cpp $(LDFLAGS)

$(VPCMODULES_OBJS) : $(OBJDIR)/%.o : $(VPCMODULES_SRCDIR)/%.cpp
	@$(CC) $(CFLAGS) -c $< -o $@

